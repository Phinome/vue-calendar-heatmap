{
  "name": "@byted-light/vue-calendar-heatmap",
  "version": "1.0.0",
  "description": "A lightweight calendar heatmap Vuejs component built on SVG, inspired by github's contribution calendar graph. base from vue-calendar-heatmap",
  "main": "dist/vue-calendar-heatmap.ssr.js",
  "browser": "dist/vue-calendar-heatmap.esm.js",
  "module": "dist/vue-calendar-heatmap.esm.js",
  "unpkg": "dist/vue-calendar-heatmap.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "v-tooltip": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@vue/cli-plugin-babel": "^4.5.3",
    "@vue/cli-service": "^4.5.3",
    "cross-env": "^7.0.2",
    "minimist": "^1.2.5",
    "rollup": "^2.26.0",
    "@rollup/plugin-babel": "^5.2.0",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-vue": "^5.1.9",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "engines": {
    "node": ">=10"
  }
}
